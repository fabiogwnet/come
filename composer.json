{
  "name": "laravel/lumen",
  "description": "The Laravel Lumen Framework.",
  "keywords": ["framework", "laravel", "lumen"],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^8.2",
    "ext-json": "*",
    "ext-memcached": "*",
    "aws/aws-sdk-php-resources": "^0.3.0",
    "doctrine/annotations": "*",
    "ebanx/stream": "^1.3",
    "fabpot/goutte": "^4.0",
    "guzzlehttp/guzzle": "^7.8",
    "illuminate/contracts": "*",
    "illuminate/pagination": "*",
    "illuminate/queue": "*",
    "illuminate/redis": "*",
    "jansenfelipe/faker-br": "^1.0",
    "laravel/lumen-framework": "^9.1",
    "lcobucci/jwt": "*",
    "league/flysystem": "^1.1",
    "mongodb/mongodb": "^1.6",
    "pearl/lumen-request-validate": "^1.4",
    "php-ffmpeg/php-ffmpeg": "*",
    "predis/predis": "^1.1",
    "ramsey/uuid": "^4.7",
    "sentry/sentry-laravel": "*",
    "spatie/flysystem-dropbox": "^1.2",
    "symfony/lock": "^5.1",
    "vimeo/vimeo-api": "^3.0",
    "watson/rememberable": "*"
  },
  "require-dev": {
    "appzcoder/lumen-routes-list": "*",
    "illuminate/support": "*",
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "nikic/php-parser": "^4.2",
    "nunomaduro/collision": "*",
    "phpro/grumphp": "^1.5",
    "phpstan/phpstan": "^1.10",
    "phpunit/phpunit": "^8.0",
    "sebastian/phpcpd": "*",
    "squizlabs/php_codesniffer": "*"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "gitlab-token": {
      "gitlab.com": "ChCbX6vwYHnYyAyALqqy"
    },
    "allow-plugins": {
      "ocramius/package-versions": true,
      "phpro/grumphp": false
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
